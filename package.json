{
  "name": "check-is-truthy",
  "version": "0.0.3",
  "description": "Utilities and types to help with simple Boolean checks",
  "keywords": [
    "boolean",
    "truthy",
    "falsy",
    "toggle",
    "predicate",
    "filter",
    "true",
    "false",
    "null",
    "undefined",
    "nan",
    "truth",
    "is-truthy",
    "istruthy",
    "is-falsy",
    "isfalsy",
    "types",
    "typescript"
  ],
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "repository": "https://github.com/ckknight/check-is-truthy",
  "author": "Cameron Knight <ckknight@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",
    "index.cjs"
  ],
  "devDependencies": {
    "@types/node": "^18.8.2",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc && tsc --declaration --emitDeclarationOnly --removeComments false && tsc --outDir cjs --module commonjs --target es5 && mv cjs/index.js index.cjs",
    "test": "node --test"
  },
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
